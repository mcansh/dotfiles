{
  "editor.fontFamily": "MonoLisa, 'Dank Mono', 'Operator Mono', 'JetBrains Mono', 'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.lineHeight": 2,
  "window.autoDetectColorScheme": true,
  "workbench.colorTheme": "Hack The Box",
  "workbench.preferredDarkColorTheme": "GitHub Dark Default",
  "workbench.preferredHighContrastColorTheme": "GitHub Dark Default",
  "workbench.preferredLightColorTheme": "GitHub Light Default",
  "editor.letterSpacing": 0.5,
  "workbench.editor.tabSizing": "shrink",
  "editor.bracketPairColorization.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "none",
  "javascript.inlayHints.parameterNames.enabled": "none",
  "editor.guides.bracketPairs": true,
  "html.format.enable": true,
  "html.format.preserveNewLines": true,
  "files.trimTrailingWhitespace": true,
  "explorer.compactFolders": false,
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.wordWrap": "on",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.fontWeight": "400",
  "editor.cursorBlinking": "solid",
  "editor.colorDecorators": false,
  "workbench.activityBar.visible": true,
  "workbench.startupEditor": "newUntitledFile",
  "extensions.ignoreRecommendations": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "workbench.editor.tabCloseButton": "right",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "workbench.editor.openSideBySideDirection": "right",
  "workbench.panel.defaultLocation": "bottom",
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.snippetSuggestions": "inline",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "window.title": "${dirty} ${activeEditorMedium}${separator}${rootName}",
  "atomKeymap.promptV3Features": true,
  "editor.formatOnSave": true,
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.packageManager": "npm",
  "eslint.options": {
    "extensions": [
      "html",
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "yaml"
    ]
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    "typescript",
    "typescriptreact"
  ],
  "color-highlight.markerType": "background",
  "color-highlight.matchWords": true,
  "files.insertFinalNewline": true,
  "git.enableCommitSigning": true,
  "html.format.indentInnerHtml": true,
  "javascript.format.enable": false,
  "terminal.explorerKind": "integrated",
  "terminal.external.osxExec": "iTerm",
  "terminal.integrated.fontSize": 12,
  "workbench.statusBar.visible": true,
  "editor.fontLigatures": true,
  "terminal.integrated.rendererType": "dom",
  "javascript.validate.enable": false,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": true,
  "workbench.editor.labelFormat": "short",
  "git.autofetch": true,
  "workbench.list.keyboardNavigation": "simple",
  "workbench.list.automaticKeyboardNavigation": false,
  "editor.suggestSelection": "first",
  "editor.largeFileOptimizations": true,
  "keyboard.touchbar.enabled": false,
  "cSpell.enableFiletypes": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "pug",
    "python",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "editor.cursorSurroundingLines": 3,
  "editor.autoClosingBrackets": "languageDefined",
  "editor.showUnused": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.svelte": "svelte",
    "*.dockerignore": "ignore"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "workbench.colorCustomizations": {
    "[Community Material Theme High Contrast]": {
      "editor.wordHighlightBackground": "#1f2d30",
      "editor.wordHighlightBorder": "#1f2d30",
      "editor.wordHighlightStrongBackground": "#1f2d30",
      "editor.wordHighlightStrongBorder": "#1f2d30",
      "editorBracketMatch.border": "#1f2d30",
      "editorBracketMatch.background": "#1f2d30",
      "scrollbarSlider.background": "#ffffff22",
      "editorOverviewRuler.bracketMatchForeground": "#0000",
      "editorOverviewRuler.errorForeground": "#0000",
      "editorOverviewRuler.warningForeground": "#0000",
      "editorOverviewRuler.infoForeground": "#0000",
      "editor.background": "#141e20"
    }
  },
  "workbench.iconTheme": "quill-icons-minimal",
  "vsonline.authentication.accountProvider": "Microsoft",
  "vsonline.accountProvider": "Microsoft",
  "kite.showWelcomeNotificationOnStartup": false,
  "webhint.enableTelemetry": "enabled",
  "codespaces.accountProvider": "Microsoft",
  "vscode-home-assistant.hostUrl": "http://raspberrypi.local:8123",
  "vscode-home-assistant.longLivedAccessToken": "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJpc3MiOiI1ZDJjZGI0MDkzYzA0M2NjODQwMjY3NTYwYWU1MzYyYiIsImlhdCI6MTU5Mzg4MDA4MCwiZXhwIjoxOTA5MjQwMDgwfQ.MqKx8H0ZFuLjBUGaCRmDGTTwtoHkZsHCygEWcIpsqqc",
  "go.formatTool": "goimports",
  "python.insidersChannel": "weekly",
  "python.languageServer": "Pylance",
  "editor.renderWhitespace": "trailing",
  "terminal.integrated.shell.osx": "/usr/local/bin/fish",
  "workbench.sideBar.location": "right",
  "editor.linkedEditing": true,
  "window.newWindowDimensions": "default",
  "codeQL.telemetry.enableTelemetry": true,
  "dart.previewLsp": true,
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "aws.profile": "profile:personal",
  "workbench.productIconTheme": "icons-carbon",
  "terminal.integrated.tabs.enabled": true,
  "workbench.editor.showTabs": true,
  "redhat.telemetry.enabled": false,
  "githubIssues.userCompletions.enabled": false,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": true
  },
  "editor.accessibilitySupport": "off",
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "vscode-edge-devtools.mirrorEdits": true,
  "window.zoomLevel": -1
  // "typescript.tsserver.experimental.enableProjectDiagnostics": true
  // when working using an external monitor
